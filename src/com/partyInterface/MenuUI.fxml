<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="512.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.partyInterface.MenuController">
   <children>
      <Canvas height="512.0" width="1024.0" />
      <ImageView fitHeight="512.0" fitWidth="1024.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/Menu.jpg" />
         </image>
      </ImageView>
      <Button fx:id="infoButton" layoutX="657.0" layoutY="123.0" mnemonicParsing="false" onAction="#changeToInfo" prefHeight="47.0" prefWidth="94.0" text="Info">
         <font>
            <Font name="Consolas Bold" size="24.0" />
         </font></Button>
      <Button fx:id="gameButton" layoutX="658.0" layoutY="225.0" mnemonicParsing="false" onAction="#changeToPreGame" prefHeight="41.0" prefWidth="93.0" text="Play">
         <font>
            <Font name="Consolas Bold" size="24.0" />
         </font></Button>
      <Button fx:id="basicsButton" layoutX="648.0" layoutY="327.0" mnemonicParsing="false" onAction="#changeToBasic" text="Basics">
         <font>
            <Font name="Consolas Bold" size="24.0" />
         </font></Button>
   </children>
</AnchorPane>
